{
  "title": "Fn Apps Launcher",
  "rules": [
    {
      "description": "[FnLauncher] Modifier -> Apps",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "left_control"
          },
          "to": [
            {
              "key_code": "left_control"
            }
          ],
          "to_if_alone": [
            {
              "shell_command": "open -b at.obdev.LaunchBar"
            }
          ]
        }
      ]
    },
    {
      "description": "[FnLauncher] Fn -> Apps",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "spacebar",
            "modifiers": {
              "mandatory": "fn"
            }
          },
          "to": [
            {
              "shell_command": "open -g 'hammerspoon://toggle_app?id=com.microsoft.VSCode'"
            }
          ]
        },
        {
